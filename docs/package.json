{
  "name": "ai-code-assistant",
  "displayName": "AI Code Assistant",
  "version": "1.0.0",
  "description": "A chat-based AI assistant for code generation and semantic search",
  "main": "./out/extension.js",
  "scripts": {
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "node ./out/test/runTest.js",
    "chunk": "ts-node scripts/parse-and-chunk.ts sample.js",
    "dev": "code ."
  },
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:aiCodeAssistant.searchChunks"
  ],
  "contributes": {
    "commands": [
      {
        "command": "aiCodeAssistant.searchChunks",
        "title": "AI: Search Code Chunks"
      }
    ]
  },
  "dependencies": {
    "node-fetch": "^2.7.0",
    "tree-sitter": "0.22.4",
    "tree-sitter-javascript": "0.23.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@types/node-fetch": "^2.6.12",
    "@types/vscode": "^1.99.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vscode": "^1.1.37"
  }
}
